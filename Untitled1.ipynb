   "source": [
    "# семей с количеством детей больше 2х не мало, выделим их в категорию \"Многодетные\"\n",
    "def children_category(child):\n",
    "    if child == 1:\n",
    "                return '1 ребенок'\n",
    "    if child == 2:\n",
    "        return '2 ребенка'\n",
    "    if child >= 3:\n",
    "        return 'Многодетные'\n",
    "    return 'Нет детей'\n",
    "#считаем отношение в процентах между числами\n",
    "def make_proportion(proc):\n",
    "    return str(round((proc.sum() / proc.count()) * 100, 2)) + '%'\n",
    "# считаем сколько всего семей с детьми каждого возраста\n",
    "data['children_category'] = data['children'].apply(children_category)\n",
    "data['children_category'].value_counts() \n",
    "#объединим в сводную таблицу\n",
    "data_pivot = data.pivot_table(index=['children_category'], values=[\"debt\"], aggfunc=['sum', 'count', make_proportion])\n",
    "# сортируем по возрастанию процента просрочек\n",
    "data_pivot = data_pivot.sort_values(by=('make_proportion', 'debt'))\n",
    "data_pivot"
   ]
